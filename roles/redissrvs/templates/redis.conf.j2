####### redis server 配置信息
port {{ item }}
maxmemory 4gb
protected-mode no
daemonize yes
dir /root/test-ansible/redis/{{ item }}/data
pidfile /root/test-ansible/redis/{{ item }}/data/redis-{{ item }}.pid

logfile /root/test-ansible/redis/{{ item }}/logs/redis-{{ item }}.log
tcp-backlog 511
timeout 300
tcp-keepalive 60
loglevel notice
cluster-enabled yes
cluster-node-timeout 15000
cluster-config-file "/root/test-ansible/redis/{{ item }}/nodes-{{ item }}.conf"